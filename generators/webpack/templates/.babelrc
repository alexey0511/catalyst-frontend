    [
      "@babel/preset-env",
      {
        "forceAllTransforms": true,
        "targets": {
        },
        "modules": false
      }
    ]<% if (options.react) { %>,
    "@babel/preset-react"<% } %>
  ],
  "plugins": ["@babel/plugin-proposal-object-rest-spread"<% if (options.react) { %> , "react-hot-loader/babel"<% } %>]<% if (options.jest) { %>,
  "env": {
    "test": {
